using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace variabler3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Mata in grader i celsius: ");
            string inmatat;
            double num = 1.8;
            int num2 = 32;
            inmatat = Console.ReadLine();
            double grader =double.Parse(inmatat);
            Console.Write("Det blir " + (grader * num + num2));
            Console.WriteLine(" grader Fahrenheit");
            
            Console.ReadLine();
        }
    }
}
